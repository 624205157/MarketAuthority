<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/background"
    android:fitsSystemWindows="true"
    >
    <include layout="@layout/title_layout"/>
    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >
        <ImageView
            android:id="@+id/background"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@mipmap/banner"
            app:layout_constraintTop_toTopOf="parent"
            />
    <android.support.constraint.ConstraintLayout
        android:id="@+id/cl_1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="15dp"
        android:background="@drawable/background_corners_1"
        android:padding="15dp"
        app:layout_constraintTop_toTopOf="parent"
        >

        <TextView
            android:id="@+id/tv_name_device"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:textColor="@color/text_color_1"
            android:text="仪器设备名称"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="20dp"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            />
        <ImageView
            app:layout_constraintRight_toLeftOf="@id/tv_name_device"
            app:layout_constraintTop_toTopOf="@id/tv_name_device"
            android:layout_marginRight="6dp"
            android:layout_marginTop="6dp"
            android:background="@drawable/background_circle"
            android:layout_width="10dp"
            android:layout_height="10dp" />
        <EditText
            android:id="@+id/et_name_device"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="120dp"
            android:background="@null"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintLeft_toRightOf="@id/tv_name_device"
            app:layout_constraintTop_toTopOf="@id/tv_name_device"
            app:layout_constraintBottom_toBottomOf="@id/tv_name_device"
            tools:text="120平"
            android:textColor="@color/title_text"
            android:textSize="16sp"
            android:padding="7dp"
            />

        <TextView
            android:id="@+id/tv_equipment_devcie"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:textColor="@color/text_color_1"
            android:text="仪器设备能力"
            android:layout_marginTop="10dp"
            app:layout_constraintLeft_toLeftOf="@id/tv_name_device"
            app:layout_constraintTop_toBottomOf="@id/tv_name_device"
            />
        <EditText
            android:id="@+id/et_equipment_devcie"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@null"
            app:layout_constraintRight_toRightOf="@id/et_name_device"
            app:layout_constraintLeft_toLeftOf="@id/et_name_device"
            app:layout_constraintLeft_toRightOf="@id/tv_equipment_devcie"
            app:layout_constraintTop_toTopOf="@id/tv_equipment_devcie"
            app:layout_constraintBottom_toBottomOf="@id/tv_equipment_devcie"
            tools:text="120平"
            android:textColor="@color/title_text"
            android:textSize="16sp"
            android:padding="7dp"
            />
        <TextView
            android:id="@+id/tv_count_device"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:textColor="@color/text_color_1"
            android:text="仪器设备数"
            android:layout_marginTop="10dp"
            app:layout_constraintLeft_toLeftOf="@id/tv_equipment_devcie"
            app:layout_constraintTop_toBottomOf="@id/tv_equipment_devcie"
            />
        <EditText
            android:id="@+id/et_count_device"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@null"
            app:layout_constraintRight_toRightOf="@id/et_name_device"
            app:layout_constraintLeft_toLeftOf="@id/et_name_device"
            app:layout_constraintLeft_toRightOf="@id/tv_count_device"
            app:layout_constraintTop_toTopOf="@id/tv_count_device"
            app:layout_constraintBottom_toBottomOf="@id/tv_count_device"
            tools:text="120平"
            android:textColor="@color/title_text"
            android:textSize="16sp"
            android:padding="7dp"
            />
        <RadioGroup
            android:id="@+id/radio_result"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:orientation="vertical"
            android:layout_marginLeft="20dp"
            app:layout_constraintLeft_toRightOf="@id/et_equipment_devcie"
            app:layout_constraintTop_toTopOf="@id/et_equipment_devcie"
            >
            <RadioButton
                android:id="@+id/regular"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                android:buttonTint="@color/red"
                android:paddingTop="3dp"
                android:paddingRight="15dp"
                android:paddingBottom="3dp"
                android:text="合格"
                android:textColor="@color/title_text"
                android:textSize="16sp" />

            <RadioButton
                android:id="@+id/unregular"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="10dp"
                android:buttonTint="@color/red"
                android:paddingTop="3dp"
                android:paddingRight="15dp"
                android:paddingBottom="3dp"
                android:textColor="@color/title_text"
                android:text="不合格"
                android:textSize="16sp"
                android:visibility="visible"
                />
        </RadioGroup>
        <View
            android:id="@+id/line1"
            app:layout_constraintTop_toBottomOf="@id/et_count_device"
            android:layout_width="match_parent"
            android:layout_marginTop="20dp"
            android:layout_height="1dp"
            android:background="@color/line_color_1"/>
        <TextView
            android:id="@+id/tv_takephoto"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginBottom="10dp"
            android:text="现场拍摄"
            android:textColor="@color/title_text"
            android:textSize="16sp"
            app:layout_constraintLeft_toLeftOf="@id/line1"
            app:layout_constraintTop_toBottomOf="@id/line1" />
        <android.support.v7.widget.RecyclerView
            android:id="@+id/recycler"
            app:layout_constraintTop_toTopOf="@id/tv_takephoto"
            app:layout_constraintBottom_toBottomOf="@id/tv_takephoto"
            android:overScrollMode="never"
            app:layout_constraintRight_toRightOf="parent"
            android:layout_marginRight="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >
        </android.support.v7.widget.RecyclerView>
    </android.support.constraint.ConstraintLayout>


